{
  "name": "workspace-template",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/types",
    "packages/example-package"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:types": "turbo run build:types",
    "build:packages": "turbo run build:packages",
    "prepare": "node scripts/prepare-publish.js",
    "postpublish": "node scripts/restore-workspace.js",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:clickhouse": "cd packages/clickhouse && pnpm test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@qiwi/multi-semantic-release": "^7.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "printWidth": 160,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "dependencies": {
    "@clickhouse/client": "^1.10.0",
    "mdxld": "^0.1.0"
  }
}
