{
  "name": "@mdxdb/clickhouse",
  "version": "0.1.0",
  "description": "ClickHouse provider for MDXDB",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:packages": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint 'src/**/*.ts'"
  },
  "dependencies": {
    "@clickhouse/client-web": "^0.2.6",
    "@mdxdb/types": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^1.1.3",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0"
  }
}
