{
  "name": "@mdxdb/server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    }
  },
  "scripts": {
    "dev": "tsx watch src/node.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/node.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "@hono/node-server": "^1.4.0",
    "@hono/zod-validator": "^0.1.11",
    "@mdxdb/clickhouse": "^0.0.1",
    "@mdxdb/fs": "^0.1.0",
    "@mdxdb/types": "^0.0.1",
    "esbuild-wasm": "^0.19.0",
    "hono": "^3.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@types/estree": "^1.0.6",
    "@types/mdx": "^2.0.13",
    "@types/react": "^18.3.17",
    "esbuild": "^0.19.12",
    "react": "^18.3.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vfile": "^6.0.3",
    "vitest": "^1.1.0",
    "wrangler": "^3.22.1"
  }
}
